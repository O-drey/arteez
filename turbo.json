{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**", "./vercel/**"],
      "env": ["NODE_ENV_API_PROD"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
